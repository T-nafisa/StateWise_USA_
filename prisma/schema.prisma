generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DailySnapshot {
  id           Int      @id @default(autoincrement())
  date         DateTime @default(now())
  subwayStatus String
  airQuality   String
  eventInfo    String
  userNote     String?
}
